<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="45"
            android:toDegrees="90"
            android:pivotX="100%"
            android:pivotY="100%">
            <shape
                android:shape="rectangle" >
                <solid
                    android:color="#fff" />
            </shape>
        </rotate>
    </item>
</layer-list>